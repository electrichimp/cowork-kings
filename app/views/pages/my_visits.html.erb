<div class="banner" style="background-image: linear-gradient(rgba(0,0,0,0.4),rgba(0,0,0,0.4)), url(https://images.unsplash.com/photo-1632489031533-62cf8cea9d2a?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=2071&q=80);">
  <div class="container">
    <h1>My <strong>visits</strong>!</h1>
  </div>
</div>
<div class="container">
  <h2 class="my-5">Tus reservas</h2>
  <div class="cards">
    <% @bookings.each do |booking| %>
      <div class="card-trip">
        <% if booking.coworking.photo.present? %>
          <%= cl_image_tag booking.coworking.photo.key %>
        <% else %>
          <img src="https://images.unsplash.com/photo-1528508492773-4d8157f3bbfa?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=2049&q=80" />
        <% end %>
        <div class="card-trip-infos">
          <div>
            <h2><%= link_to booking.coworking.name, coworking_path(booking) %></h2>
            <p>rentado por <%= booking.user.email %></p>
          </div>
          <h2 class="card-trip-pricing">$<%= booking.coworking.price.round(2) %></h2>
          <img src="https://kitt.lewagon.com/placeholder/users/krokrob" class="card-trip-user avatar-bordered" />
          </div>
          <%= link_to "Cancelar reserva", booking_path(booking), method: :delete, class: "btn btn-primary" %>
      </div>
    <% end %>
  </div>
</div>
