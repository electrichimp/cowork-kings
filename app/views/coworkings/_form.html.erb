<%= simple_form_for(coworking) do |f| %>
  <%= f.input :name %>
  <%= f.input :address, class: "coworking_address" %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :capacity %>
  <%= f.input :photo, as: :file %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.button :submit, class: "btn-primary" %>
<% end %>
